<h1>Add a contact</h1>
<%= form_tag '/contacts', method: :post do %>
  <div>
    <%= label_tag :first_name %>
    <%= text_field_tag :first_name %>
  </div>
  <div>
    <%= label_tag :last_name %> 
    <%= text_field_tag :last_name %>
  </div>
  <div>
    <%= label_tag :occupation %>
    <%= text_field_tag :occupation %>
  </div>
  <div>
    <%= label_tag :fav_food %>
    <%= text_field_tag :fav_food %>
  </div>
  <div>
    <%= label_tag :address %>
    <%= text_field_tag :addresss %>
  </div>
  <div>
    <%= submit_tag "Add Contact" %>
  </div>
<% end %>